          "v.enderman = q.count([q.get_nearby_entities(25, 'minecraft:enderman')]);",
          "v.endermite = q.count([q.get_nearby_entities(25, 'minecraft:endermite')]);",
          "v.evoker = q.count([q.get_nearby_entities(25, 'minecraft:evoker')]);",
          "v.ghast = q.count([q.get_nearby_entities(25, 'minecraft:ghast')]);",
          "v.guardian = q.count([q.get_nearby_entities(25, 'minecraft:guardian')]);",
          "v.hoglin = q.count([q.get_nearby_entities(25, 'minecraft:hoglin')]);",
