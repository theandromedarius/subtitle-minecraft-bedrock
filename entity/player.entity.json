        "object.enderman": "geometry.object.enderman",
        "object.pillager": "geometry.object.pillager",
        "object.witch": "geometry.object.witch"
      },
      "scripts": {
        "scale": "0.9375",
        "initialize": [
          "variable.is_holding_right = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;"
        ],
        "pre_animation": [
          "v.blaze = q.count([q.get_nearby_entities(25, 'minecraft:blaze')]);",
          "v.creeper = q.count([q.get_nearby_entities(25, 'minecraft:creeper')]);",
          "v.drowned = q.count([q.get_nearby_entities(25, 'minecraft:drowned')]);",
          "v.elder_guardian = q.count([q.get_nearby_entities(25, 'minecraft:elder_guardian')]);",
